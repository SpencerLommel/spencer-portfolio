		pmem@0xbdef0000 {
			no-map;
			reg = <0x00 0xbdef0000 0x00 0x10000>;
		};

		ramoops@0xbdf00000 {
			compatible = "ramoops";
			reg = <0x00 0xbdf00000 0x00 0x100000>;
